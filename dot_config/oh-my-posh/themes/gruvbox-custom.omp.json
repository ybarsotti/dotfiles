{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "transient_prompt": {
    "foreground": "#d5c4a1",
    "background": "transparent",
    "template": "{{ .UserName }}@{{ .HostName }} {{ .Folder | path }} {{ if .Root }}#{{ else }}${{ end }} "
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "diamond",
          "foreground": "#1d2021",
          "background": "#fabd2f",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "template": " {{ if .SSHSession }}\uf817 {{ end }}{{ .UserName }}@{{ .HostName }} "
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#1d2021",
          "background": "#fe8019",
          "properties": {
            "style": "folder",
            "max_depth": 3,
            "folder_separator_icon": " \ue0b1 "
          },
          "template": " \uf07c {{ .Path }} "
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#1d2021",
          "background": "#8ec07c",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#d79921{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#fb4934{{ end }}",
            "{{ if gt .Ahead 0 }}#b8bb26{{ end }}",
            "{{ if gt .Behind 0 }}#fb4934{{ end }}"
          ],
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} "
        },
        {
          "type": "node",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#1d2021",
          "background": "#689d6a",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "properties": {
            "fetch_version": true
          }
        },
        {
          "type": "python",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#1d2021",
          "background": "#458588",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": true
          }
        },
        {
          "type": "go",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#1d2021",
          "background": "#83a598",
          "template": " \ue626 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "properties": {
            "fetch_version": true
          }
        },
        {
          "type": "rust",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#1d2021",
          "background": "#d3869b",
          "template": " \ue7a8 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "properties": {
            "fetch_version": true
          }
        },
        {
          "type": "docker",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#1d2021",
          "background": "#458588",
          "template": " \uf308 {{ .Context }} ",
          "properties": {
            "display_default": false
          }
        },
        {
          "type": "kubectl",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#1d2021",
          "background": "#b16286",
          "template": " \uf308 {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}} ",
          "properties": {
            "display_error": false
          }
        },
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "#d5c4a1",
          "properties": {
            "threshold": 5000,
            "style": "austin"
          },
          "template": " <#928374>\uf252</> {{ .FormattedMs }}"
        },
        {
          "type": "status",
          "style": "diamond",
          "foreground": "#1d2021",
          "background": "#8ec07c",
          "background_templates": [
            "{{ if gt .Code 0 }}#fb4934{{ end }}"
          ],
          "leading_diamond": " ",
          "trailing_diamond": "\ue0b4",
          "template": " {{ if gt .Code 0 }}\uf00d {{ reason .Code }}{{ else }}\uf00c{{ end }} ",
          "properties": {
            "always_enabled": true
          }
        }
      ]
    },
    {
      "type": "rprompt",
      "segments": [
        {
          "type": "shell",
          "style": "plain",
          "foreground": "#928374",
          "template": "{{ .Name }} "
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "#928374",
          "template": "{{ .CurrentDate | date .Format }}",
          "properties": {
            "time_format": "15:04:05"
          }
        }
      ]
    }
  ]
}